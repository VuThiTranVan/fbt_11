<div class="col-md-4 visit-grid">
  <div class="item-1 item-type-spin">
    <div class="item-img">
      <%= image_tag travel.images.first.picture.url, class: "img-responsive" %>
    </div>
  </div>
  <h4><%= link_to travel.name, travel %></h4>
  <div class="visit-blog">
    <div class="visit-btn">
      <%= render "ratings/show", travel: travel %>
    </div>
    <div class="visit-likes">
      <span><%= t "tour_duration" %>: <%= travel.tour_duration %></span>
    </div>
  </div>
  <div class="clearfix"> </div>
</div>
